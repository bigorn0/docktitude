_docktitude()
{
  local cur prev
  COMPREPLY=()
  cur="${COMP_WORDS[COMP_CWORD]}"
  prev="${COMP_WORDS[COMP_CWORD-1]}"
  case $prev in
    'docktitude')
      COMPREPLY=( $(compgen -W "build clean config export help info op print script snapshot status tree update upgrade version" -- $cur) )
      return 0
      ;;
    'build'|'print'|'script')
      COMPREPLY=( $(compgen -f -o plusdirs -- $cur) )
      return 0
      ;;
  esac
  return 0
}
complete -F _docktitude docktitude
